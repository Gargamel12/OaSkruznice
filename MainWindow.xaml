        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:O_a_S_kružnice"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="MainWindow">
        <Button x:Name="click" Content="Vypočítat" HorizontalAlignment="Left" Height="36" Margin="186,181,0,0" VerticalAlignment="Top" Width="396"/>
        <TextBox x:Name="hodnota" HorizontalAlignment="Left" Height="36" Margin="186,128,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="111"/>
        <ComboBox x:Name="volba" HorizontalAlignment="Left" Height="36" Margin="302,128,0,0" VerticalAlignment="Top" Width="82">
            <ComboBoxItem Content="d"/>
            <ComboBoxItem Content="r"/>
        </ComboBox>
        <TextBox x:Name="obsah" HorizontalAlignment="Left" Height="36" Margin="399,128,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="94"/>
        <TextBox x:Name="obvod" HorizontalAlignment="Left" Height="36" Margin="498,128,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="84"/>

    </Grid>
</Window>
